   <conceptSet>
      <concept id="digsrctypedigitalCapture" modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:digitalCapture" created="2008-01-01T01:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Digital capture sampled from real life</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">The media was captured from a real-life source using a digital camera or digital recording device</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypecomputationalCapture"
               modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:computationalCapture"
                    created="2024-09-17T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Multi-frame computational capture sampled from real life</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">The media is the result of capturing multiple frames from a real-life source using a digital camera or digital recording device, then automatically merging them into a single frame using digital signal processing techniques and/or non-generative AI. Includes High Dynamic Range (HDR) processing common in smartphone camera apps.</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypenegativeFilm" modified="2024-09-26T12:00:00+00:00">
         <conceptId qcode="digsrctype:negativeFilm" created="2008-01-01T01:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-26T12:00:00+00:00">Digitised from a transparent negative</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">The media was digitised from a negative on film or other transparent medium</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypepositiveFilm" modified="2024-09-26T12:00:00+00:00">
         <conceptId qcode="digsrctype:positiveFilm" created="2008-01-01T01:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-26T12:00:00+00:00">Digitised from a transparent positive</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">The media was digitised from a positive on a transparency or other transparent medium</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypeprint" modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:print" created="2008-01-01T01:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-26T12:00:00+00:00">Digitised from a non-transparent medium</name>
         <definition xml:lang="en-GB" modified="2024-09-26T12:00:00+00:00">The media was digitised from a non-transparent medium such as a photographic print</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypeminorHumanEdits" modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:minorHumanEdits"
                    created="2022-06-14T12:00:00+00:00"
                    retired="2024-09-17T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Original media with minor human edits</name>
         <definition xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Minor augmentation or correction by a human, such as a digitally-retouched photo used in a magazine</definition>
         <note xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Retired. Use "humanEdits" instead.</note>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypehumanEdits" modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:humanEdits" created="2024-09-17T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Human-edited media</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Augmentation, correction or enhancement by one or more humans using non-generative tools</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypecompositeWithTrainedAlgorithmicMedia"
               modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:compositeWithTrainedAlgorithmicMedia"
                    created="2023-07-26T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Edited using Generative AI</name>
         <definition xml:lang="en-GB" modified="2023-07-26T12:00:00+00:00">Augmentation, correction or enhancement using a Generative AI model, such as with inpainting or outpainting operations</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypealgorithmicallyEnhanced"
               modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:algorithmicallyEnhanced"
                    created="2022-06-14T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Algorithmically-altered media</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Modification or correction by algorithm without changing the main content of the media, initiated or configured by a human, such as sharpening or applying noise reduction</definition>
         <note xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Can be used when tlhe digital media is the result of capturing, processing, and merging one or more frames from a real-life source using a digital camera or digital recording device. Processing may include conventional digital signal processing techniques and/or machine learning models that are not designed to change the semantic contents of the scene.</note>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypesoftwareImage" modified="2022-06-14T12:00:00+00:00">
         <conceptId qcode="digsrctype:softwareImage"
                    created="2008-01-01T01:00:00+00:00"
                    retired="2022-06-14T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB">Created by software</name>
         <definition xml:lang="en-GB">The digital image was created by computer software</definition>
         <note xml:lang="en-GB">Retired in June 2022. Use more specific terms instead.</note>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypedigitalArt" modified="2022-06-14T12:00:00+00:00">
         <conceptId qcode="digsrctype:digitalArt"
                    created="2022-06-14T12:00:00+00:00"
                    retired="2024-09-17T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Digital art</name>
         <definition xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Media created by a human using digital tools</definition>
         <note xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Retired. Use "digitalCreation" instead.</note>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypedigitalCreation" modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:digitalCreation" created="2024-09-17T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Digital creation</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Media created by a human using non-generative tools</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypedataDrivenMedia" modified="2022-06-14T12:00:00+00:00">
         <conceptId qcode="digsrctype:dataDrivenMedia" created="2022-06-14T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Data-driven media</name>
         <definition xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Digital media representation of data via human programming or creativity</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypetrainedAlgorithmicMedia"
               modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:trainedAlgorithmicMedia"
                    created="2022-06-14T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Created using Generative AI</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Digital media created algorithmically using an Artificial Intellgence model trained on captured content</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypealgorithmicMedia" modified="2022-06-14T12:00:00+00:00">
         <conceptId qcode="digsrctype:algorithmicMedia" created="2022-06-14T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Pure algorithmic media</name>
         <definition xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Media created purely by an algorithm not based on any sampled training data, e.g. an image created by software using a mathematical formula</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypescreenCapture" modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:screenCapture" created="2024-09-17T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Screen capture</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">A capture of the contents of the screen of a computer or mobile device</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypevirtualRecording" modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:virtualRecording" created="2022-06-14T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Virtual event recording</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Live recording of virtual event based on Generative AI and/or captured elements</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypecomposite" modified="2024-09-17T12:00:00+00:00">
         <conceptId qcode="digsrctype:composite" created="2024-09-17T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Composite of elements</name>
         <definition xml:lang="en-GB" modified="2024-09-17T12:00:00+00:00">Mix or composite of several elements, any of which may or may not be generative AI</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypecompositeCapture" modified="2022-06-14T12:00:00+00:00">
         <conceptId qcode="digsrctype:compositeCapture" created="2022-06-14T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Composite of captured elements</name>
         <definition xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Mix or composite of several elements that are all captures of real life</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
      <concept id="digsrctypecompositeSynthetic" modified="2022-06-14T12:00:00+00:00">
         <conceptId qcode="digsrctype:compositeSynthetic"
                    created="2022-06-14T12:00:00+00:00"/>
         <type qcode="cpnat:abstract"/>
         <name xml:lang="en-GB" modified="2022-08-01T12:00:00+00:00">Composite including generative AI elements</name>
         <definition xml:lang="en-GB" modified="2022-06-14T12:00:00+00:00">Mix or composite of several elements, at least one of which is Generative AI</definition>
         <related uri="http://cv.iptc.org/newscodes/digitalsourcetype/"
                  rel="skos:inScheme"/>
      </concept>
   </conceptSet>
